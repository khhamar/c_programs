#include <stdio.h>
void moveNegatives(int arr[], int n) {
    int left = 0; 
    int right = n - 1;  
    while (left <= right) {
        if (arr[left] < 0 && arr[right] >= 0) {
            int temp = arr[left];
            arr[left] = arr[right];
            arr[right] = temp;
            left++;
            right--;
        } else if (arr[left] >= 0) {
            left++;
        } else if (arr[right] < 0) {
            right--;
        }
    }
}
int main() {
    int arr[] = {-12, 11, -13, -5, 6, -7, 5, -3, -6};
    int n = sizeof(arr) / sizeof(arr[0]);
    printf("Original array: ");
    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");
    moveNegatives(arr, n);
    printf("Array after moving negatives to one side: ");
    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");
    return 0;
}
